{
  "name": "cooljs.me",
  "version": "1.0.0",
  "description": "personal blog system",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elvisz/cooljs.me.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "Elvis Zhong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Elvisz/cooljs.me/issues"
  },
  "homepage": "https://github.com/Elvisz/cooljs.me#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "awesome-typescript-loader": "^5.0.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.3.2",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "raf": "^3.4.0",
    "react": "^16.3.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.1",
    "react-router": "^4.2.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^22.4.2",
    "ts-loader": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^3.0.3",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-dev-server": "^3.1.3",
    "webpack-manifest-plugin": "^2.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/transform/typescript.js",
      "^.+\\.css$": "<rootDir>/config/jest/transform/css.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/transform/file.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "mjs",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "<rootDir>/config/jest/tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
